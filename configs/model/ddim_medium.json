{
    "model": {
        "name": "unet2d",
        "input_size": 32,
        "in_channels": 1,
        "out_channels": 1,
        "layers_per_block": 1,
        "block_out_channels": [64, 128, 256, 512],
        "norm_num_groups": 32,
        "down_block_types": ["DownBlock2D","DownBlock2D","DownBlock2D","AttnDownBlock2D"],
        "up_block_types": ["AttnUpBlock2D","UpBlock2D","UpBlock2D","UpBlock2D"]
    },
    "scheduler": {
        "name": "DDIM",
        "train_timesteps": 1000
    },
    "optimizer": {
        "name": "adam",
        "lr": 0.0002
    }
}